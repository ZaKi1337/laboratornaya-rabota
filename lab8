#include <stdio.h>
#define YES 1
#define NO 0
#define MAXLINE 500

int main(void)
{
	int flag = NO, found = NO;	//флаг слова, флаг удаления
	char line[MAXLINE], *ptr1, *ptr2, *ptr3, c, prevc;

	fgets(line, MAXLINE - 1, stdin);	//сохраняем строку, введенную с клавиатуры, в массив
	ptr1 = line;	//указатель на текущий входной символ
	ptr2 = line;	//указатель на текущий выходной символ
	ptr3 = line;	//указатель на начало слова
	prevc = ' ';	//предыдущий символ

	//цикл чтения символов из массива
	//повторяется пока не найдем конец строки
	do {
		c = *ptr1;	//запоминаем текущий символ
		if (c == ' ' || c == '.' || c == ',' || c == '\n' || c == '\0')
		{
			// если найден разделитель
			if (flag == YES)
				//если только что кончилось слово
				if (found == NO)
					//если не было двух подряд идущих гласных
					//слово переносится в результирующую строку
					while (ptr3 != ptr1)
					{
						*ptr2 = *ptr3;	//записываем в выходную строку текущую букву слова
						ptr2++;	//переходим к следующему символу выходной строки
						ptr3++;	//переходим к следующему букве в слове
					}

			found = NO;	//опускаем флаг удаления
			flag = NO;	//опускаем флаг слова
			*ptr2 = c;	//записываем в выходную строку текущий символ
			ptr2++;		//переходим к следующему символу выходной строки
		}
		else
		{
			//если буква
			if (flag == NO)
				//если первая буква в слове
				ptr3 = ptr1;	//запоминаем адрес начала слова

			if ((c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' || c == 'y' ||
				 c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U' || c == 'Y') &&
				(prevc == 'a' || prevc == 'e' || prevc == 'i' || prevc == 'o' || prevc == 'u' || prevc == 'y' ||
				 prevc == 'A' || prevc == 'E' || prevc == 'I' || prevc == 'O' || prevc == 'U' || prevc == 'Y'))
				//если текущая и предыдущая буквы гласные
				found = YES;	//поднимаем флаг удаления

			flag = YES;		//поднимаем флаг слова
		}
		prevc = c;	//запоминаем предыдущий символ
		ptr1++;		//переходим к следующему символу входной строки
	} while (c != '\0');

	fputs(line, stdout);	//выводим получившуюся строку
	getchar();
	return 0;
}
